{
  "name": "react-typescript-checkmark",
  "description": "A TypeScript safe, color customizable, animated checkmark for React.",
  "version": "1.0.3",
  "license": "Apache-2.0",
  "homepage": "https://github.com/davidwilson3/react-typescript-checkmark",
  "author": "David Wilson (https://github.com/davidwilson3)",
  "main": "dist/checkmark.js",
  "types": "dist/checkmark.d.ts",
  "scripts": {
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:dist": "webpack --env=prod --progress --profile --colors",
    "build": "npm run build:dist && npm run build:dev",
    "watch": "webpack --env=prod --progress --watch",
    "dev": "webpack-dev-server --env=dev",
    "start": "concurrently --kill-others \"npm run watch\" \"npm run dev\"",
    "tsc": "tsc ./src/checkmark.tsx --jsx react"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.5.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "loglevel": "^1.4.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "style-loader": "^0.23.1",
    "styled-components": "^5.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.1.0"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "files": [
    "LICENSE",
    "package.json",
    "README.md",
    "dist"
  ]
}
