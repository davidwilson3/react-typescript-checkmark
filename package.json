    "@types/styled-components": "^5.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.5.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "loglevel": "^1.4.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "style-loader": "^0.23.1",
    "styled-components": "^5.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.1.0"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "files": [
    "LICENSE",
    "package.json",
    "README.md",
    "dist"
